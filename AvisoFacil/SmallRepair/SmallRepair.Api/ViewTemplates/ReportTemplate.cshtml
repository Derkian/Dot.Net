<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<style>
    body {
        font-family: "Roboto", "Helvetica", "Arial", sans-serif;
    }

    h4 {
        color: #36aba6;
        font-size: 20px !important;
        font-weight: 400 !important;
        border-left: 3px solid #36aba6;
        padding-left: 10px;
        font-family: "Roboto", "Helvetica", "Arial", sans-serif;
    }

    p {
        font-family: "Roboto", "Helvetica", "Arial", sans-serif;
    }

    table {
        width: 100%;
        display: table;
        border-spacing: 0;
        border-collapse: collapse;
    }

    thead {
        background-color: #bdbdbd;
    }

    tr {
        color: inherit;
        display: table-row;
        outline: 0;
        vertical-align: middle;
        border-color: inherit;
    }

    th {
        display: table-cell;
        padding: 14px 13px 14px 6px;
        font-size: 12px;
        border-bottom: 1px solid rgba(224, 224, 224, 1);
        letter-spacing: 0.01071em;
        vertical-align: inherit;
        color: #fff;
        font-family: "Roboto", "Helvetica", "Arial", sans-serif;
    }

    td {
        padding: 8px 5px 7px 6px;
        font-size: 12px;
        text-align: left;
        font-family: "Roboto", "Helvetica", "Arial", sans-serif;
        font-weight: 400;
        line-height: 1.43;
        border-bottom: 1px solid rgba(224, 224, 224, 1);
        text-transform: uppercase;
    }

    .textCenter {
        text-align: center;
    }

    .textLeft {
        text-align: left;
    }

    .textRight {
        text-align: right;
    }

    .services {
        background-color: #f3f3f3;
    }

        .services td {
            padding: 6px 5px;
            font-size: 11px;
        }

    header {
        display: flex;
        justify-content: space-between;
        align-items: center;
    }

    .col-4 {
        padding: 8px;
        flex-grow: 0;
        max-width: 33.3%;
        flex-basis: 33.3%;
    }

    .col-3 {
        padding: 8px;
        flex-grow: 0;
        max-width: 25%;
        flex-basis: 25%;
    }

    .col-6 {
        padding: 8px;
        flex-grow: 0;
        max-width: 50%;
        flex-basis: 50%;
    }

    .valor {
        width: 100%;
        display: flex;
        align-items: center;
        justify-content: center;
        background-color: #36aba6;
        color: #fff;
        font-size: 28px;
        font-weight: 400;
        height: 50px;
        border-radius: 4px;
    }
</style>
<body>
    <header>
        <div class="col-4"><img style="max-width: 100%" src="data:image/jpeg;base64," alt="logo" /></div>
        <div class="col-6">
            <h4>DADOS DO ORÇAMENTO</h4>
            <p style="padding: 0 16px;"><b>CLIENTE:</b> @Model.Assessment.Company.Name </p>
            <p style="padding: 0 16px;"><b>PLACA:</b> @Model.Assessment.Plate</p>
            <p style="padding: 0 16px;"><b>VEÍCULO:</b> @Model.Assessment.Model</p>
        </div>
    </header>
    <section>
        <h4>DANOS</h4>
        <table>
            <thead>
                <tr>
                    <th class="textLeft">Dano</th>
                    <th class="textLeft">Intensidade</th>
                    <th class="textLeft">Peça</th>
                    <th class="textCenter">Horas</th>
                    <th class="textCenter">Valor Serviço</th>
                    <th class="textCenter">Material de Pintura</th>
                    <th class="textRight">Total</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var part in @Model.Assessment.Parts)
                {
                    <tr>
                        @switch ((System.Int32)@part.MalfunctionType)
                        {
                            case 0:
                                <td></td>
                                break;
                            case 1:
                                <td>Risco</td>
                                break;
                            case 2:
                                <td>Amassado</td>
                                break;
                            case 3:
                                <td>Granizo</td>
                                break;
                            case 4:
                                <td>Martelinho de Ouro</td>
                                break;
                            default:
                                <td></td>
                                break;
                        }

                        @switch ((System.Int32)@part.IntensityType)
                        {
                            case 0:
                                <td></td>
                                break;
                            case 1:
                                <td>Leve</td>
                                break;
                            case 2:
                                <td>Médio</td>
                                break;
                            case 3:
                                <td>Forte</td>
                                break;
                            default:
                                <td></td>
                                break;
                        }

                        <td>@part.Description</td>
                        <td class="textCenter">@part.TotalTime</td>
                        <td class="textCenter">@part.TotalService.ToString("N2")</td>
                        <td class="textCenter">@part.TotalMaterial.ToString("N2")</td>
                        <td class="textRight">@part.Total.ToString("N2")</td>
                    </tr>

                    <!-- Condicional -->

                    @foreach (var service in part.Services)
                    {
                        <tr class="services">
                            <td colspan="2"></td>

                            @switch ((System.Int32)service.ServiceType)
                            {
                                case 1:
                                    <td>Pintura</td>
                                    break;
                                case 2:
                                    <td>Recuperação</td>
                                    break;
                                default:
                                    <td></td>
                                    break;
                            }
                            <td class="textCenter">@service.Time</td>
                            <td class="textCenter">@service.Value.ToString("N2")</td>
                            <td class="textCenter">@service.MaterialValue.ToString("N2")</td>
                            <td class="textRight">@service.Total.ToString("N2")</td>
                        </tr>
                    }

                    <!-- Fim Condicional -->
                }
            </tbody>
        </table>
    </section>
    <section>
        @if (Model.Assessment.AdditionalServices.Count > 0)
        {
            <h4>ADICIONAIS</h4>
            <table>
                <thead>
                    <tr>
                        <th class="textLeft">Descrição</th>
                        <th class="textRight">Total</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var additionalService in Model.Assessment.AdditionalServices)
                    {
                        <tr>
                            <td>@additionalService.Description</td>
                            <td class="textRight">@additionalService.Value.ToString("N2")</td>
                        </tr>

                    }
                </tbody>
            </table>
        }
    </section>
    <section>
        <h4>RESUMO</h4>
        <table>
            <thead>
                <tr>
                    <th class="textLeft">Mão de Obra</th>
                    <th class="textCenter">Preço</th>
                    <th class="textCenter">Horas</th>
                    <th class="textRight">Total</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var summary in Model.Summaries)
                {
                    <tr>
                        @switch (summary.Description.ToUpper())
                        {
                            case "PAINTING":
                                <td>Pintura</td>
                                break;
                            case "REPAIR":
                                <td>Recuperação</td>
                                break;
                            case "PARTS":
                                <td>Peças</td>
                                break;
                            case "ADDITIONALSERVICE":
                                <td>Serviços Adicionais</td>
                                break;
                            case "MATERIAL":
                                <td>Material de Pintura</td>
                                break;
                            default:
                                <td></td>
                                break;
                        }

                        @if (@summary.ValuePerHour != null)
                        {
                            <td class="textCenter">@summary.ValuePerHour.ToString("N2")</td>
                        }
                        else
                        {
                            <td class="textCenter"></td>
                        }

                        @if (@summary.AmountHours != null)
                        {
                            <td class="textCenter">@summary.AmountHours</td>
                        }
                        else
                        {
                            <td class="textCenter"></td>
                        }

                        @if (@summary.ServiceType != null)
                        {
                            <td class="textRight">@summary.TotalService.ToString("N2")</td>
                        }
                        else
                        {
                            <td class="textRight">@summary.Total.ToString("N2")</td>
                        }
                    </tr>
                }

                <tr>
                    <td colspan="3"><b>Valor Total</b></td>
                    <td class="textRight"><b>@Model.Assessment.Total.ToString("N2")</b></td>
                </tr>
            </tbody>
        </table>
    </section>
</body>
